{
  "name": "tsxecharts",
  "version": "1.0.7",
  "description": "React component wrapper for ECharts based on TypeScript.",
  "repository": {
    "type": "git",
    "url": "https://github.com/lloydzhou/tsxecharts.git"
  },
  "author": "lloydzhou",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "typescript",
    "tsx",
    "echarts"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "module": "lib/index.js",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "echarts": "^5.4.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "resize-detector": "^0.3.0",
    "typescript": "^4.8.4",
    "web-vitals": "^2.1.4"
  },
  "peerDependencies": {
    "echarts": "^5.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resize-detector": "^0.3.0"
  },
  "scripts": {
    "lib": "tsc --jsx react-jsx src/lib/index.ts --esModuleInterop --skipLibCheck --allowSyntheticDefaultImports -t es5 -d --outDir lib",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier --write src/**/*.ts src/**/*.tsx src/**/*.css src/*.tsx"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "**/*.{ts,tsx,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  }
}
